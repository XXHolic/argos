  "compilerOptions": {
    "outDir": "es6",
    "noImplicitAny": false,
    "noImplicitThis": false,
    "module": "es6",
    "target": "es6",
    "allowJs": true,
    // "sourceMap": true,
    // "moduleResolution":"node",
    "strict": false,
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}